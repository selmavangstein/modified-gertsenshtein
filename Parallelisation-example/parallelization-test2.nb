Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"DefManifold", "[", 
    RowBox[{"M", ",", "4", ",", 
     RowBox[{"IndexRange", "[", 
      RowBox[{"{", 
       RowBox[{"a", ",", "s"}], "}"}], "]"}]}], "]"}], ";"}], " ", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"might", " ", "fix", " ", 
     RowBox[{"formatting", ":", " ", 
      RowBox[{"type", " ", "roman"}]}]}], ",", " ", 
    RowBox[{"look", " ", 
     RowBox[{"greek", ".", " ", "See"}], " ", "DefManifold", " ", 
