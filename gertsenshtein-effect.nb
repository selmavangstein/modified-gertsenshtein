      RowBox[{"LinearLagrangian", "=", 
       RowBox[{"LinearLagrangian", "/.", "ToOrder"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"SecondOrderPart", "=", 
       RowBox[{"LinearLagrangian", "//", 
