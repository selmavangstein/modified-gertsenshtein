Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"ExtractQuadraticPart", "[", "InputExpr_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"LinearLagrangian", "=", "InputExpr"}], ",", "SecondOrderPart",
