       RowBox[{"SecondOrderPart", "/.", 
        RowBox[{"PerturbativeParameter", "->", "1"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"SecondOrderPart", "//=", "ToCanonical"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"SecondOrderPart", "//=", "ContractMetric"}], ";", 
